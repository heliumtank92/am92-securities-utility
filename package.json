      "email": "jadhavsuyash2@gmail.com",
      "url": "https://github.com/jsuyash"
    }
  ],
  "dependencies": {},
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "securities",
    "Sec Master"
  ]
